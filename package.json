{
  "name": "fake-sanity-client",
  "version": "0.0.0",
  "description": "Fake (in-memory) Sanity client for testing.",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/mariuslundgard/fake-sanity-client#readme",
  "bugs": {
    "url": "https://github.com/mariuslundgard/fake-sanity-client/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mariuslundgard/fake-sanity-client.git"
  },
  "license": "MIT",
  "author": "Marius Lundg√•rd <studio@mariuslundgard.com>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.cjs",
      "node": {
        "import": "./dist/index.cjs.js",
        "require": "./dist/index.cjs"
      },
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "run-s clean pkg:build pkg:check",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "lint": "eslint .",
    "pkg:build": "pkg-utils build --strict",
    "pkg:check": "pkg-utils --strict",
    "pkg:watch": "pkg-utils watch --strict",
    "prepare": "husky install",
    "prepublishOnly": "run-s build",
    "test": "run-s vitest:test",
    "test:browser": "echo 'TODO'",
    "type-check": "tsc --build",
    "vitest:test": "vitest run",
    "watch": "run-s pkg:watch"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ]
  },
  "prettier": {
    "bracketSpacing": false,
    "plugins": [
      "prettier-plugin-packagejson"
    ],
    "printWidth": 100,
    "semi": false,
    "singleQuote": true
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "extends": "@sanity/semantic-release-preset"
  },
  "dependencies": {
    "diff-match-patch": "^1.0.5",
    "get-it": "^8.1.3",
    "groq-js": "^1.1.9",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@sanity/client": "^6.1.2",
    "@sanity/pkg-utils": "2.2.15",
    "@sanity/semantic-release-preset": "^4.1.1",
    "@types/node": "^20.2.4",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prettier-plugin-packagejson": "^2.4.3",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "semantic-release": "^21.0.2",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1"
  },
  "peerDependencies": {
    "@sanity/client": "^6"
  },
  "packageManager": "pnpm@8.5.1",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "pnpm": {
    "overrides": {
      "rxjs": "^7.8.0"
    }
  }
}
